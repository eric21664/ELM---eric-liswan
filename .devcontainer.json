{
  "customizations": {
    "vscode": {
      "extensions": [
        "Elmtooling.elm-ls-vscode"
      ]
    },
    "forwardPorts": [
      5000,
      5900,
      6081,
      8080,
      8082,
      8787
    ],
    "mounts": [
      {
        "source": "/var/run/docker.sock",
        "target": "/var/run/docker-host.sock",
        "type": "bind"
      }
    ]
  },
  "image": "docker.io/bohrium2b/elm:v1"
}
